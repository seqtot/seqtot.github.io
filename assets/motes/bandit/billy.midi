<settings>
  pitchShift: -1;
  @drums: v50; $bass: v50; $bassS: v60; $guit: v50; $back: v50; $solo: v50; $Z: v50;

<out b105>
> СЧЁТ      %_ [4*120]
> проход    %A [4*120]
> РИФФ      %B [12*120_4*120]
> РИФФ      %C [12*120_4*120]
> КУПЛЕТ    %D [16*120]
> КУПЛЕТ    %E [16*120]
> РИФФ      %F [12*120_4*120]
> РИФФ      %G [12*120_4*120]
> КУПЛЕТ    %H [16*120]
> КУПЛЕТ    %I [16*120]
> сбивка    %J [16*120]
> соло_бас  %K [16*120]
> соло_бас  %L [16*120]
> СОЛО_2_1  %M [16*120_16*120]
> КОДА      %O [5*120]
> ТИШИНА    %z [2*4*120]

<РИФФ b105 set>
оЯюЯ:r3
проход

<КОДА b105 set>
оЯюЯ:r3
проход_кода

<проход b105 $>
$solo: де=90 ла=90 ма=60 фа=90 ма=90 да=60 $trump

<проход_кода b105 $>
$solo: да=30 60 ло=30 60 мо=30 30 фо=30 60 мо=30 60 до=30 30 лу=30 90 $trump
$back: де=30 60 ла=30 60 ма=30 30 фа=30 60 ма=30 60 да=30 30 ло=30 90 $xylo

<оЯюЯ b105 $>
$guit: лу=90 мо+ло=60 лу=30 мо+ло=60 до=90 ро=90 мо=60 $egit*cl
$bass: лы=60 лы=60 лы=60 лы=60 лы=60 лы=60 лы=60 лы=60 $eBass*f

<куплет_соло b105 $>
$guit: 0    лу=90 до=90 ро=60 мо=90 ро=90 до=60 $egit*cl
$guit: 480  лу=90 до=90 ро=60 фо=90 мо=90 до=60 $egit*cl
$guit: 960  лу=90 до=90 ро=60 мо=90 ро=90 до=60 $egit*cl
$guit: 1440 ро=90 до=90 бу=60 до=90 бу=90 су=60 $egit*cl
# 8Q   1920

<КУПЛЕТ b105 set>
куплет_бас куплет_фон куплет_соло

<куплет_бас b105 $> #1920
$bass: 0    лы=90 му=90 му=60 ду=90 ду=90 му=60 $eBass*f
$bass: 480  лы=90 му=90 му=60 фу=90 ду=90 му=60 $eBass*f
$bass: 960  лы=90 му=90 му=60 ду=90 ду=90 му=60 $eBass*f
$bass: 1440 ру=90 ру=90 бы=60 ду=90 ду=90 сы=60 $eBass*f

<куплет_фон b105 $>
$back: 0    лу=480    лу=480    лу=480    ро=240 до=240  $organ
# 8Q   1920

<соло_бас b105 $>
$bassS: 0       лу=90 мо=90 до=40 20  бу=90 со=90 бу=50 $cBass*f
$bassS: 480     до=90 мо=90 до=40 20  ро=90 мо=90 фо=50 $cBass*f
$bassS: 960     мо=90 до=90 су=40 20  ро=90 бу=90 су=50 $cBass*f
$bassS: 1440    мо=90 до=90 су=40 20  зу=90 бу=90 мо=50 $cBass*f
# 8Q       1920

<сбивка b105 $>
$guit: 0    лу+мо+ло=30 лу+мо+ло=30 *drpm лу=30 лу=30 лу=30 лу=30 лу=30 лу=30    $egit*drp
$guit: 240  бу+мо+ло=30 бу+мо+ло=30 *drpm бу=30 бу=30 бу=30 бу=30 бу=30 бу=30    $egit*drp
$guit: 480  до+мо+да=30 до+мо+да=30 *drpm до=30 до=30 до=30 до=30 до=30 до=30    $egit*drp
$guit: 720  то+мо+ло=30 то+мо+ло=30 *drpm то=30 то=30 то=30 то=30 то=30 то=30    $egit*drp
$guit: 960  ро+ло+ра=30 ро+ло+ра=30 *drpm ро=30 ро=30 ро=30 ро=30 ро=30 ро=30    $egit*drp
$guit: 1200 но+ло+на=30 но+ло+на=30 *drpm но=30 но=30 но=30 но=30 но=30 но=30    $egit*drp
$guit: 1440 мо+бо+ма=30 мо+бо+ма=30 *drpm мо=30 мо=30 мо=30 мо=30 мо=30 мо=30    $egit*drp
$guit: 1680 мо+бо=30     *drpm ро=30      ро=30 до=30 до=30 бу=30 бу=30 зу=30    $egit*drp

<СОЛО_2_1 b105 set>
соло2_бас соло2_соло1
соло2_бас соло2_соло2

<соло2_бас b105 $>
$bass: 0   лы=90 ду=90 лы=60  бы=90 ру=90 бы=60  ду=90 му=90 ду=60  ру=90 фу=90 ру=60 $eBass*f
$bass: 960 ду=90 му=90 ду=60  бы=90 ру=90 бы=60  ду=90 му=90 ду=60  му=90 зу=90 му=60 $eBass*f

<соло2_соло1 b105 $>
$solo: 120      ма=120 ма=120 ра=60 да=180 d480 $trump
$back: 0        лу=240 бу=240 $organ
$solo: 480 180  да=60 ра=60 ма=60 фа=120 d480 $trump
$back: 480      до=240 ро=240 $organ
$solo: 960      ма=90 30 да=120 да=120 бо=60 да=180 d480 $trump
$back: 960      до=240 бу=240 $organ
$solo: 1440 180 бо=60 да=60 ра=60 да=60 бо=60 d480 $trump
$back: 1440     до=240 мо=240 $organ

<соло2_соло2 b105 $>
$solo: ло=120   ма=120 ма=120 ра=60 да=180 d480 $trump
$back: 0        лу=240 бу=240 $organ
$solo: 480 180  да=60 ра=60 ма=60 фа=120 d480 $trump
$back: 480      до=240 ро=240 $organ
$solo: 960      ма=90 30 да=120 да=120 бо=120 d480 $trump
$back: 960      до=240 бу=240 $organ
$solo: 1440     да=30 ра=30 да=180 120 бо=120  d480 $trump
$back: 1440     до=240 мо=240 $organ

<СЧЁТ b105 $>
$bass: ле=60 60 ле=60 60 ле=60 60 ле=60 60 $eBass*f
$guit: ле=60 60 ле=60 60 ле=60 60 ле=60 60 $egit*cl

<ТИШИНА b120 $> #960
$Z: 960 d960 $xylo
